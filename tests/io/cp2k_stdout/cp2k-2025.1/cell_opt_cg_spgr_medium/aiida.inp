&GLOBAL
  RUN_TYPE CELL_OPT
  PRINT_LEVEL MEDIUM
  EXTENDED_FFT_LENGTHS .TRUE.
  PREFERRED_DIAG_LIBRARY SCALAPACK
  PROJECT aiida
&END GLOBAL
&MOTION
  &CELL_OPT
    &CG
      &LINE_SEARCH
        TYPE 2PNT
      &END LINE_SEARCH
    &END CG
    &BFGS
      TRUST_RADIUS 0.0075
    &END BFGS
    TYPE DIRECT_CELL_OPT
    MAX_ITER 25
    MAX_FORCE 9.7e-05
    OPTIMIZER CG
    KEEP_ANGLES .TRUE.
    EPS_SYMMETRY 0.005
    KEEP_SPACE_GROUP .TRUE.
    PRESSURE_TOLERANCE 100.0
  &END CELL_OPT
&END MOTION
&FORCE_EVAL
  &DFT
    &QS
      EPS_DEFAULT 1e-14
      EXTRAPOLATION USE_GUESS
    &END QS
    &XC
      &XC_FUNCTIONAL
        &MGGA_X_SCAN
        &END MGGA_X_SCAN
        &MGGA_C_SCAN
        &END MGGA_C_SCAN
      &END XC_FUNCTIONAL
    &END XC
    &SCF
      &SMEAR .FALSE.
      &END SMEAR
      &MIXING
        BETA 0.5
        ALPHA 0.5
        METHOD BROYDEN_MIXING
        NBUFFER 10
      &END MIXING
      EPS_SCF 5e-07
      MAX_SCF 50
      ADDED_MOS 72
      SCF_GUESS RESTART
    &END SCF
    &MGRID
      CUTOFF 550.0
      NGRIDS 4
      REL_CUTOFF 100.0
    &END MGRID
    &KPOINTS
      SCHEME MONKHORST-PACK 6 4 4
      EPS_GEO 1e-08
      SYMMETRY .FALSE.
      FULL_GRID .FALSE.
    &END KPOINTS
    &POISSON
      PERIODIC XYZ
      POISSON_SOLVER PERIODIC
    &END POISSON
    BASIS_SET_FILE_NAME BASIS_MOLOPT_UCL
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME POTENTIAL_UZH
